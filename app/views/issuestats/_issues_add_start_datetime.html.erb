<% if @project.module_enabled?("issuestats") == true %>
  <p>
    <% @issue_start_time = IssueStartTime.find_by_issue_id(params[:id]) %>
    <%= label :issuestats, "Start time" %> <%= text_field :issuestats, :start_time, :type => "time" %>
    <% if @issue_start_time != nil %>
    <%= @issue_start_time.start_datetime %>
    <% end %>
<% end %>
